check process <%= @worker_name %>
  with pidfile /data/<%= @app_name %>/shared/pids/job_runner.pid
  start program = "/data/<%= @app_name %>/current/script/delayed_job start -- <%= @framework_env %>" 
  stop program = "/data/<%= @app_name %>/current/script/delayed_job stop -- <%= @framework_env %>"
  group <%= @app_name %>_jobs